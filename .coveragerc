[run]
branch = False
source =
    cms
    menus
omit =
    */migrations/*
    */commands/*
    */tests/*
    cms/test_utils/*
    docs/*
    env/*
    env*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.: